alias django-runserver='python ~/Git/chatbot/manage.py runserver 0.0.0.0:8000'
alias django-makemigrationsmigrate='python ~/Git/chatbot/manage.py makemigrations && python ~/Git/chatbot/manage.py migrate'
alias django-venv-windows='source ~/Git/chatbot/venv/Scripts/activate'
alias django-venv-unix='source ~/Git/chatbot/venv/bin/activate'
